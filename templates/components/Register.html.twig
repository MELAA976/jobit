<div{{attributes}} class ="form" form>
<div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    {{ form_start(form)}}

         {{ form_row(form.roles) }}
         {{ form_row(form.nom) }}

         {% if form.prenom is defined %}
         {{ form_row(form.prenom) }}
        {% endif %}

        {% if form.date_naissance is defined %}
         {{ form_row(form.date_naissance) }}
        {% endif %}

        {% if form.photo is defined %}
         {{ form_row(form.photo) }}
        {% endif %}

        {{ form_row(form.email) }}

        {{ form_row(form.plainPassword,{label: 'Password' }) }}
         

        {% if form.adresse is defined %}
         {{ form_row(form.adresse) }}
        {% endif %}

        {% if form.telephone is defined %}
         {{ form_row(form.telephone) }}
        {% endif %}

        

        {% if form.logo is defined %}
         {{ form_row(form.logo) }}
        {% endif %}

        {% if form.presentation is defined %}
         {{ form_row(form.presentation) }}
        {% endif %}

        {% if form.site_web is defined %}
         {{ form_row(form.site_web) }}
        {% endif %}

        {% if (form.entreprise) is defined %}
         {{ form_row(form.entreprise) }}
         <turbo-frame id= "newEntreprise"> 
        <a href="{{path('app_entreprise_new')}}">Ajouter une entreprise</a>
        </turbo-frame>
         
        {% endif %}
        {{ form_row(form.agreeTerms) }}

        <button type="submit" class="btn">Enregister</button>
    {{ form_end(form) }}
</div>

{#{ form_errors(registrationForm) }}

    {{ form_start(registrationForm)}}
         {{ form_row(registrationForm.roles) }}
        {{ form_row(registrationForm.nom) }}
        {{ form_row(registrationForm.prenom) }}
        {{ form_row(registrationForm.date_naissance) }}
        {{ form_row(registrationForm.telephone) }}
        {{ form_row(registrationForm.photo) }}
        {{ form_row(registrationForm.presentation) }}
        {{ form_row(registrationForm.site_web) }}
        {{ form_row(registrationForm.logo) }}
        {{ form_row(registrationForm.adresse) }}
        {{ form_row(registrationForm.email) }}
        {{ form_row(registrationForm.plainPassword,{label: 'Password' }) }}
        {{form_row(registrationForm.entreprise)}}
        {{ form_row(registrationForm.agreeTerms) }}

        <button type="submit" class="btn">Register</button>
    {{ form_end(registrationForm) }#}